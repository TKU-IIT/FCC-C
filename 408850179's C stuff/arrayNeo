#include<stdio.h>

int main(int argc, char **argv){
	int v[20], c, n, user_terminate;
	
	for (c=0, n=0, user_terminate=0; c<20, user_terminate==0; c++{
		scanf("%d", &(v[c]));
		if (v[c]>0){
			n++;
		}else{
			user_terminate++;
		}
	}
	
	for(n; n>=20; n--){//EX:for(c=n-1; c>=0; c--){printf("%d"), v[c]);}//
		printf("%d", v[n]);
	}
	
	printf("\n");
	
	return 0;
}