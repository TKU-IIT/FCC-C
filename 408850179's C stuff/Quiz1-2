#include <stdio.h>

int main(int argc, char **argv) {
    int n1, n2, larger, smaller, counter;

    scanf("%d", &n1);
    scanf("%d", &n2);

    if(n1>n2) {
        larger = n1;
        smaller = n2;
    }else{
        larger = n2;
        smaller = n1;
    }
    printf("%d ", larger);
    printf("%d", smaller);

    printf("\n");

    counter = larger;
    while(counter >= smaller){
        printf("%d", counter);
        printf(" ");
        counter = counter-1; 
    }

    return 0;
}