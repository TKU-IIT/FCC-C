#include<stdio.h>

void printf_row(int W){
	int col_c
	
	col_c = 0;
	while(col_c < W){
		printf("*");
		col_c = col_c + 1;
	}
	printf("\n");
}

int main(int argc, char **argv){
	int W, H, row_c, col_c;
	
	scanf("%d", &W);
	scanf("%d", &H);
	
	row_c = 0;
	while(row_c < H){
		col_c = 0;
		while(col_c < W){
			printf("*");
			col_c = col_c + 1;
		}
		printf("\n");
		row_c = row_c + 1;
	}
	
	return 0 ;
}